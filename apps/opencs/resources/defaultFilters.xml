<!DOCTYPE FilterTree>
<SetOperation type="Union">
    <Default active="false">
      <Name>Default</Name>
    </Default>
    
    <Match type="Wildcard" active="false">
	<Name>Smoke</Name>
        <Key>mwId</Key>
        <Value>*smoke*</Value>
    </Match>
    
    <SetOperation type="Union">
	<Match type="Exact" active="false">
	    <Name>Activator</Name>
            <Key>mwType</Key>
            <Value>ACTI</Value>
        </Match>
        <Match type="Exact">
	    <Name>Potion</Name>
            <Key>mwType</Key>
            <Value>ALCH</Value>
        </Match>
        <Match type="Exact" active="false">
	    <Name>Script</Name>
            <Key>mwType</Key>
            <Value>SCPT</Value>
        </Match>
    </SetOperation>
    
    <SetOperation type="Intersection" active="false">
      <Name>Activators with Sound</Name>
      <Match type="Exact">
	<Name>Activator</Name>
	<Key>mwType</Key>
	<Value>ACTI</Value>
      </Match>
      <Match type="Regex">
	<Name>Sound</Name>
        <Key>script</Key>
        <Value>Sound.*</Value>
      </Match>
    </SetOperation>
    <SetOperation/>
</SetOperation>

